            foreach ($objects as $object) {
                if ($object != "." && $object != "..") {
                    if (filetype($dir.DIRECTORY_SEPARATOR.$object) == "dir")
                        $result=$result&&$this->_rmdir($dir.DIRECTORY_SEPARATOR.$object,$result);
                    else
                        $result=$result&&unlink($dir.DIRECTORY_SEPARATOR.$object);
                }
