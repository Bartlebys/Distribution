	$path=str_replace(FLEXIONS_ROOT_DIR,"", $path);
	return $path;
}



function injectVersionInPath($path){
	$h=Hypotypose::Instance();
	$newPath= str_ireplace('{version.major}',$h->majorVersionPathSegmentString(),$path);
	return $newPath;
