class  TriggerForIndexes extends MongoEndPoint {

    function call() {
        /* @var TriggerForIndexesCallData */
        $parameters=$this->getModel();
        $db=$this->getDB();
        /* @var \MongoCollection */
        $collection = $db->triggers;
        $indexes=$parameters->getValueForKey(TriggerForIndexesCallData::indexes);

