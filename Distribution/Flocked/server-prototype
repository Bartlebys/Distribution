   $_creatorUID_operation_rep->type=FlexionsTypes::STRING;
   $_creatorUID_operation_rep->required=true;
   $_creatorUID_operation_rep->default="\\(Default.NO_UID)";
   $_creatorUID_operation_rep->isGeneratedType=true;
   
