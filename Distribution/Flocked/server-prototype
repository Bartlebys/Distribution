    function call() {
        /* @var BartlebySyncRemoveGhostsCallData */
        $parameters=$this->getModel();
        $this->ioManager = $this->getIoManager();
        $details = $this->ioManager->removeGhosts();
        return new JsonResponse($details, 201);
    }
