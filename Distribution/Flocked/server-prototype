        if (isset($spaceUID)){
            return 'value="'.$spaceUID.'" ';
        }else{
            return '';
        }
    }

    function  valueForUserUID(){
        $userUID=$this->getModel()->getValueForKey(SignInCallData::userUID);
        if (isset($userUID)){
