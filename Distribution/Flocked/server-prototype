 */
abstract class Enum {

    /**
     * @return array
     */
    static function possibleValues() {
        returnarray();
    }

    /**
     * @return true if the value is a member of the Enum
     */
    final function isValid($value) {
        $a = $this->possibleValues();
        $r = in_array($value, $a);
        return $r;
    }

