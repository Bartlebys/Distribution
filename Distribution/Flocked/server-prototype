            FlexionsTypes::OBJECT,
            FlexionsTypes::COLLECTION,
            FlexionsTypes::ENUM,
            FlexionsTypes::NOT_SUPPORTED,
            FlexionsTypes::VOID,
            FlexionsTypes::DATA
        );
    }
