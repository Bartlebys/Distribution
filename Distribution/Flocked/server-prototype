    $actionString = 'creation';
    $localAction = 'upsert';
} elseif ($httpMethod == "PUT") {
    $actionString = 'update';
    $localAction = 'upsert';
} elseif ($httpMethod == "PATCH") {
    $actionString = 'update';
    $localAction = 'upsert';
