       define('REPOSITORY_HOST','https://<REPOSITORY_HOST>');
       define('REPOSITORY_WRITING_PATH', dirname(__DIR__) .DIRECTORY_SEPARATOR.'files'.DIRECTORY_SEPARATOR);
