    $includeBlock .= stringIndent("import Alamofire", 1);
    $includeBlock .= stringIndent("import ObjectMapper", 1);
} else {
    $includeBlock .= stringIndent("import Alamofire", 1);
    $includeBlock .= stringIndent("import ObjectMapper", 1);
    $includeBlock .= stringIndent("import BartlebyKit", 1);
